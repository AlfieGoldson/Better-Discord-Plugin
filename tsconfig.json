{
    "compilerOptions": {
        "module": "esnext",
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "target": "ES2020",
        "lib": ["ESNext", "DOM"],
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "jsx": "react",
        "jsxFactory": "window.BdApi.React.createElement"
    },
    "include": ["src", "@types/index.ts"]
}
